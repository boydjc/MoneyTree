SELECT SYMBOL,datetime(TRADE_TIME_IN_LONG/1000, 'unixepoch', 'localtime') AS Trade_Time,
	   BID_PRICE, ROUND(AVG(BID_PRICE) OVER (ORDER BY ID ROWS BETWEEN 1 PRECEDING AND 2 FOLLOWING),2) as BID_PRICE_MOV_AVG,
	   BID_SIZE, ROUND(AVG(BID_SIZE) OVER (ORDER BY ID ROWS BETWEEN 1 PRECEDING AND 2 FOLLOWING),2) as BID_SIZE_MOV_AVG,
	   ASK_PRICE, ROUND(AVG(ASK_PRICE) OVER (ORDER BY ID ROWS BETWEEN 1 PRECEDING AND 2 FOLLOWING),2) as ASK_PRICE_MOV_AVG,
	   ASK_SIZE, ROUND(AVG(ASK_SIZE) OVER (ORDER BY ID ROWS BETWEEN 1 PRECEDING AND 2 FOLLOWING), 2) as ASK_SIZE_MOV_AVG,LAST_PRICE,
	   LAST_SIZE FROM DTST WHERE SYMBOL != "None";